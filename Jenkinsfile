






pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
      }

    stages {
        stage('build') {
            steps {
                echo 'Hello build'
                sleep5
            }
       }
            

        stage('deploy') {
            steps {
                echo 'Hello deploy'
            }
         }

    stages {
        stage('test') {
            steps {
                echo 'Hello test'
               sleep4
            }
        }
    }
 }
